[[["var", "ES", "Si", "foo"]
 ,["fun", "E", ["F", "Si", "Si"], "bar"]]]


BEGIN Function definition may not share external declaration with declarators (2)
int bar(int x) {
}, foo;
PARSE
EXPECT_MESSAGE
:2:2: error unexpected token ','
EXPECT_AST
[
    ["fun", "E", ["F", "Si", "Si"], "bar", ["x"], ["block", []]],
    0
]


BEGIN Parameters have function prototype scope (1)
