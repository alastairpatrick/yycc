    q * v; // now "q" is a type so this declares variable "v" with type "q*".
}
EXPECT_AST
[["block", [
    ["*", "Nq", "Np"],
    ["typedef", "Ss", "q"],
    ["var", [], "PSs", "v"]
]]]


